{
    "name": "dukens/openai-suggestions",
    "description": "Use the OpenAI API together with Laravel Ignition to show AI-powered fixes for errors in sage/acorn.",
    "license": "MIT",
    "type": "package",
    "autoload": {
        "psr-4": {
            "Dukens\\OpenaiSuggestions\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Dukens Thelemaque",
            "email": "dukens@radishlab.com"
        }
    ],
    "require": {
        "php": "^8.1.0",
        "illuminate/support": "^9.0",
        "openai-php/client": "^0.6.1",
        "spatie/laravel-ignition": "^1.6.4|^2.0"
    },
    "extra": {
        "acorn": {
            "providers": [
                "Dukens\\OpenaiSuggestions\\OpenAiSolutionServiceProvider"
            ]
        }
    }

}
